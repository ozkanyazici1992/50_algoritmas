   "source": [
    "def num_split(num):\n",
    "    count = 0\n",
    "    result=[]\n",
    "    for i in str(abs(num))[::-1]:\n",
    "        result.append(int(i)*10**count)\n",
    "        count+=1\n",
    "    return result[::-1] if num >=0 else [-i for i in result[::-1]]"
   ]
